
. ./tools.config

VERSION=1.1.11

wget -q http://www.cbcb.umd.edu/software/jellyfish/jellyfish-${VERSION}.tar.gz && \
tar xzf jellyfish-${VERSION}.tar.gz && \
cd jellyfish-${VERSION} && \
./configure --prefix=${PREFIX} && \
make && \
make check && \
make install-strip && \
cd .. && \
rm -rf jellyfish-${VERSION} jellyfish-${VERSION}.tar.gz

